def find_max(numbers):
    if not numbers:
        return "List is empty"
    max_number = numbers[0]
    for num in numbers:
        if num > max_number:
            max_number = num
    return max_number
num_list = [int(x) for x in input("Enter numbers separated by space: ").split()]
max_element = find_max(num_list)
print("The maximum element in the list is:", max_element)
