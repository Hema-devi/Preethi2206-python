def sort_dictionary_by_keys(dictionary):
    sorted_items = sorted(dictionary.items(), key=lambda x: x[0])
    sorted_dict = dict(sorted_items)
    
    return sorted_dict
my_dict = {'c': 3, 'a': 1, 'b': 2}
sorted_dict = sort_dictionary_by_keys(my_dict)
print("Sorted dictionary by keys:", sorted_dict)
