def remove_duplicates(input_string):
    unique_chars = set()
    result = ''
    for char in input_string:
        if char not in unique_chars:
            result += char
            unique_chars.add(char)
    return result
input_str = "hello"
result_str = remove_duplicates(input_str)
print(result_str)  # Output: "helo"
