def get_first_half_months(months_tuple):
    half_length = len(months_tuple) // 2
    first_half = months_tuple[:half_length]
    return first_half
months_tuple = ("January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December")
first_half_months = get_first_half_months(months_tuple)
print("First half of months:", first_half_months)
